+                success: true,
+                data: {
+                  finalStatus: result.status,
+                  statusUpdates: statusUpdates,
+                  completed: true
+                }
+              })
+            },
+            onError: (error) => {
+              realtimeService.stopPolling(testEsim.id)
+              resolve({
+                success: false,
+                error: error.error || 'Monitoring failed',
+                data: { statusUpdates }
+              })
+            },
+            pollInterval: 2000, // 2 seconds for testing
+            maxAttempts: 5 // 10 seconds total
+          })
+          
