+
+  // Helper function to group usage by day
+  groupUsageByDay(usageRecords) {
+    const dailyUsage = {}
+
+    usageRecords.forEach(record => {
+      const date = new Date(record.timestamp).toISOString().split('T')[0]
+      if (!dailyUsage[date]) {
+        dailyUsage[date] = 0
+      }
+      dailyUsage[date] += record.data_used || 0
+    })
+
+    return dailyUsage
+  },
